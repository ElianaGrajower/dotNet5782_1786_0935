<Window x:Class="PL.AccountWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PL"
        mc:Ignorable="d"
        Title="AccountWindow" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/inline_image_preview.jpg" Opacity="0.8"/>
        </Grid.Background>
        <Button x:Name="viewAccount" Content="view account" HorizontalAlignment="Left" Margin="178,191,0,0" VerticalAlignment="Top" Height="69" Width="117" Click="viewAccount_Click"/>
        <Button x:Name="parcels" Content="parcels" HorizontalAlignment="Left" Margin="383,191,0,0" VerticalAlignment="Top" Height="69" Width="117" Click="parcels_Click"/>
        <Button Content="log out" HorizontalAlignment="Left" Margin="605,68,0,0" VerticalAlignment="Top" Height="42" Width="96" Click="Button_Click"/>

    </Grid>
</Window>
